<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Model</th>
      <th>Chassis number</th>
      <th>Engine number</th>
      <th>Plate</th>
      <th>Kilometers</th>
      <th>Sell date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.branch %></td>
        <td><%= car.model %></td>
        <td><%= car.chassis_number %></td>
        <td><%= car.engine_number %></td>
        <td><%= car.plate %></td>
        <td><%= car.kilometers %></td>
        <td><%= car.sell_date %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
